`timescale 1ns / 1ps

module test_usart(CLK, 
                  CLR, 
                  CLR_Rec, 
                  Data_Tx, 
                  Rx, 
                  CLK_B, 
                  Data_Ready, 
                  Data_Rx, 
                  parity_err, 
                  Tx);

    input CLK;
    input CLR;
    input CLR_Rec;
    input [7:0] Data_Tx;
    input Rx;
   output CLK_B;
   output Data_Ready;
   output [7:0] Data_Rx;
   output parity_err;
   output Tx;
   
   wire CLK_B_DUMMY;
   
   assign CLK_B = CLK_B_DUMMY;
   Sync_Reciver  Reciever (.CLK(CLK), 
                         .CLK_Baud(CLK_B_DUMMY), 
                         .CLR(CLR_Rec), 
                         .Serial_input(Rx), 
                         .Data(Data_Rx[7:0]), 
                         .Data_Ready(Data_Ready), 
                         .Parity_ERR(parity_err));
   Sync_Transmitter_Baud  Transmitter (.CLK(CLK), 
                                  .CLR(CLR), 
                                  .Data(Data_Tx[7:0]), 
                                  .RST(CLR_Rec), 
                                  .CLK_Baud(CLK_B_DUMMY), 
                                  .OUT_ser(Tx));
endmodule
